{
  "name": "@metafam/discord-bot",
  "private": true,
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "yarn generate && tsc -b",
    "dev": "concurrently \"yarn dev-ts\" \"yarn generate --watch\"",
    "dev-ts": "ts-node-dev --exit-child --respawn -- src/index.ts",
    "typecheck": "yarn build",
    "precommit": "yarn lint-staged",
    "generate": "graphql-codegen --config=codegen.yml",
    "lintfix": "eslint --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@typeit/discord": "4.0.9",
    "discord.js": "12.5.1",
    "dotenv": "8.2.0",
    "ethers": "4.0.48",
    "graphql": "15.4.0",
    "graphql-tag": "2.11.0",
    "node-fetch": "2.6.1",
    "graphql-request": "3.2.0",
    "sourcecred": "0.7.6"
  },
  "devDependencies": {
    "@types/node-fetch": "2.5.7",
    "concurrently": "5.3.0",
    "ts-node-dev": "1.1.1"
  }
}
