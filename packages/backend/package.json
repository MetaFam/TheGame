    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.0.15",
    "node-fetch": "^2.6.0",
    "graphql-request": "^3.0.0-next.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node-fetch": "^2.5.6",
    "concurrently": "^5.2.0",
    "ts-node-dev": "^1.0.0-pre.56"
